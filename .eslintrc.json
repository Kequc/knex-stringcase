{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "prefer-template": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-use-before-define": "off",
        "arrow-parens": "off",
        "global-require": "off",
        "quote-props": "off",
        "no-plusplus": "off",
        "no-useless-escape": "off"
    }
}